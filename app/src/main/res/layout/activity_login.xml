<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LoginActivity">

    <ImageView
        android:id="@+id/image_login_page"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/family_in_house_icon_safe_symbol_insurance_symbol_vector_31047382"
        android:layout_margin="64dp"/>
    <Button
        android:text="Click to sign in with google"
        android:layout_width="324dp"
        android:onClick="signIn"
        android:layout_height="64dp"
        app:layout_constraintTop_toBottomOf="@+id/image_login_page"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
         />


</androidx.constraintlayout.widget.ConstraintLayout>